{
	"HTTP Strict Transport Security": {"Strict-Transport-Security": "(max\\-age=)[0-9]*(;IncludeSubDomains)"},
	"Content Security Policy": {"Content-Security-Policy": "(default-src 'self')"},
	"Cross-Site Scripting Protection": {"X-XSS-Protection": "(1; mode=block)"},
	"Clickjacking": {"X-Frame-Options": "(Deny)|(SAMEORIGIN)"},
  "Content Sniffing": {"X-Content-Type-Options": "(nosniff)"},
  "Cacheable HTTPS Response (Control)": {"Cache-Control": "(no-store)|(private, max-age=0)"},
  "Cacheable HTTPS Response (Pragma)":{"Pragma": "(no-cache)"},
  "Referrer Policy": {"Referrer-Policy": "(strict-origin-when-cross-origin)"}
}
